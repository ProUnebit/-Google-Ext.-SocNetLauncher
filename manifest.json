{
    "manifest_version": 2,
    "name": "SocNetLauncher",
    "description": "It's a personal extension - social network launcher. For Google Chrome.",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
